{
    "id": "hello",
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "jiuliano/hello:1.0",
            "privileged": true,
            "network": "HOST",
            "parameters": [
                { "key": "publish", "value": "8090:8090" }
